{
  "name": "react-ts-app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vitest run --passWithNoTests && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:css": "stylelint '**/*.scss'",
    "preview": "vite preview",
    "test": "vitest --coverage",
    "format:check": "prettier --check .",
    "format:write": "prettier --write .",
    "clean": "rimraf dist/ .temp/ coverage/",
    "clean:all": "rimraf dist/ .temp/ coverage/ && rimraf node_modules/",
    "reset": "rimraf node_modules/ && npm cache clean --force && npm install"
  },
  "dependencies": {
    "@swc/core": "^1.4.2",
    "inversify": "^6.0.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.22.1",
    "reflect-metadata": "^0.2.1"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/node": "^20.11.24",
    "@types/react": "^18.2.58",
    "@types/react-dom": "^18.2.19",
    "@typescript-eslint/eslint-plugin": "^7.0.2",
    "@typescript-eslint/parser": "^7.0.2",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "@vitest/coverage-istanbul": "^1.3.1",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "inversify-react": "^1.1.0",
    "jsdom": "^24.0.0",
    "prettier": "^3.2.5",
    "sass": "^1.71.1",
    "stylelint": "^16.2.1",
    "stylelint-config-standard-scss": "^13.0.0",
    "stylelint-scss": "^6.2.1",
    "typescript": "^5.2.2",
    "vite": "^5.1.0",
    "vite-plugin-singlefile": "^2.0.0",
    "vite-plugin-svgr": "^4.2.0",
    "vitest": "^1.3.0"
  }
}
